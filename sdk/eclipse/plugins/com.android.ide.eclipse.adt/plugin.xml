      </page>
   </extension>
   <extension
         point="org.eclipse.ui.actionSets">
      <actionSet
            description="Android Wizards"
            id="adt.actionSet.wizards"
            label="Android Wizards"
            visible="true">
         <action
               class="com.android.ide.eclipse.adt.internal.wizards.actions.NewXmlFileAction"
